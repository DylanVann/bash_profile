[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = Dylan Vann
	email = dylan.vann@gmail.com
[alias]
	s = status
	c = commit
	co = checkout
	br = branch
	undo = "! f() { git clean -df && git checkout -- . }; f"
	permission-reset = !git diff -p -R | grep -E \"^(diff|(old|new) mode)\" | git apply
	reset-to-remote = "! git clean -d -f && git fetch origin && git checkout master && git reset --hard origin/master"
[core]
	excludesfile = /Users/dylanvann/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/dylanvann/Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
	helper = osxkeychain
