[filter "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
[user]
  name = Dylan Vann
  email = dylan.vann@gmail.com
[alias]
  s = status
  c = commit
  p = push
  pf = push -f
  co = checkout
  br = branch
  undo = "! f() { git clean -df && git checkout -- . }; f"
  permission-reset = "! git diff -p -R | grep -E \"^(diff|(old|new) mode)\" | git apply"
  reset-to-remote = "! git clean -d -f && git fetch origin && git checkout master && git reset --hard origin/master"
  permission-reset = !git diff -p -R --no-color | grep -E \"^(diff|(old|new) mode)\" --color=never | git apply
[core]
  excludesfile = /Users/dylan/.gitignore_global
  editor = /usr/bin/vim
  autocrlf = input
  pager = diff-so-fancy | less --tabs=4 -RFX
[credential]
  helper = osxkeychain
[rebase]
  autostash = true
[commit]
  template = /Users/dylan/.stCommitMsg
[diff-so-fancy]
  changeHunkIndicators = true
